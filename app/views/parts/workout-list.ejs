<div class="workout-wrapper">
    <% for(let i=0; i < data.length; i++){%>
    <ul>
        <li>
            <p><% let date = new Date(data[i].start_time);
                let hours = date.getHours();
                let minutes = date.getHours();
                hours = hours < 10 ? '0' + hours : hours;
                minutes = minutes < 10 ? '0' + minutes : minutes; %>
                <%= hours + ":" + minutes %></p>
        </li>
        <li>
            <p><%= data[i].train_name %></p>
            <span><%= data[i].discription %></span>
        </li>
        <li>
            <p><%= data[i].traineer %></p>
        </li>
        <li>
            <a class="workout-link" href="<%= data[i].link_url %>" target="_blank">Подключиться</a>
        </li>
    </ul>
    <% if(!data[i].online){ %>
    <div class="workout-list">
        <h2>Список упражнений</h2>
        <div class="workout-table-wrapper">
            <table>
                <tr>
                    <th>
                       Кол-во
                       <br>
                       <span>(повторений)</span> 
                    </th>
                    <th>
                        Кол-во
                        <br>
                        <span>(подходов)</span> 
                     </th>
                     <th>
                        Упражнение
                     </th>
                     <th>
                        Статус
                     </th>
                </tr>
                <tr>
                    <td>a</td>
                    <td>b</td>
                    <td>c</td>
                    <td><input type="checkbox"></td>
                </tr>
            </table>
        </div>
    </div>
    <% } %>
    <%}%>
</div>